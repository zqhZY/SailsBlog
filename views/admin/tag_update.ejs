    <form action="/update_tag.json" method="POST" role="form" class="admin-form form-horizontal" enctype="multipart/form-data">
        <hr>
        <% _.each(updatetag, function (tag) { %>

        <div class="form-group">
            <label for="name" class="col-md-2 control-label">Tag Name
                &nbsp;
            </label>
            <div class="col-md-10">
                <input name="tagid" value="<%= tag.id %>" type="hidden">

                <input class="form-control" id="name" name="tagname" type="text"    
                    value="<%= tag.tagname %>" >

            </div>

        </div>

        <% }) %>
        <hr>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10 submit-row">
                <input class="btn btn-primary" value="Save" type="submit">


                <input name="_add_another" class="btn btn-default" value="Save and Add Another" type="submit">
                <input name="_continue_editing" class="btn btn-default" value="Save and Continue Editing" type="submit">

                <a href="/admin_tags" class="btn btn-danger" role="button">Cancel</a>

            </div>
        </div>


    </form>



